


<h1>{{users.username}}'s Summer Bucket List</h1>
<ul>
	{{#each user}}
		<li>Name: <a href="/users/{{../user.id}}/lists/{{this.id}}/edit">{{this.first_name}}</a></li>
		<form action="/users/{{../user.id}}/lists/{{this.id}}?_method=DELETE" method="POST">
			<input type="submit" value="Delete This">
		</form>
		<br>
	{{/each}}
</ul>
